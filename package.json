{
  "name": "now-enum-parser",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "source": "src/index.ts",
  "type": "module",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": "https://github.com/living-tech/now_enum_parser.git",
  "scripts": {
    "build": "npm-run-all -s clean build:cjs build:esm",
    "build:cjs": "tsc --project . --module commonjs --outDir ./dist",
    "build:esm": "tsc --project . --module es2015 --outDir ./esm",
    "clean": "rimraf ./dist ./esm",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "tsc --watch",
    "lint": "tslint -p tsconfig.json -c tslint.json"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5"
  }
}
